<%= render 'main_nav' %>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
      <th>Phone</th>
      <th>Total Texts</th>
    </tr>
  </thead>

  <tbody>
    <% @data.each do |row| %>
      <tr>
        <td><%= row[:name] %></td>
        <td><%= row[:date].in_time_zone.strftime('%_m/%d/%y') %></td>
        <td><%= number_to_phone(row[:phone], country_code: row[:country_code], delimiter: '.') %></td>
        <td><%= row[:daily_total] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
